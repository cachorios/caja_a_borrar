<div class="row-fluid">
    <div class="span6">
        {{ form_row(edit_form.nombre) }}
        {{ form_row(edit_form.empresa) }}
    </div>
    <div class="span6">
        {{ form_row(edit_form.identificador) }}
        {{ form_row(edit_form.valor) }}
        {{ form_row(edit_form.longitud) }}
    </div>
</div>
<div class="row-fluid">
    <div class="span12">
        {{ form_row(edit_form.posiciones) }}
    </div>
</div>

