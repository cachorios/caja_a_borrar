    <div class="span12">
        <div style="padding-bottom: 24px;" class="bs-docs-example">
            <a rel="popover" class="btn btn-large btn-danger" href="#" data-original-title="A Title"
               data-content2="And here's some amasasa asaazing content. It's very engaging. right?">Click to toggle
                popover</a>

           
        </div>
        {{ form_row(edit_form.vtos_importes) }}
    </div>
</div>

<div class="row-fluid">
    <div class="span12">
        {{ form_row(edit_form.observacion) }}
        {{ form_widget(edit_form) }}
        <div id="luis-rios">

            <a href="#" class="btn"
               data-coleccion-add-btn="#caja_sistemacajabundle_codigobarratype_vtos_importes_control_group"> test </a>
