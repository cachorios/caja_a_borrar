{% extends '::layout.html.twig' %}
{% block id  "mod_usuario_ingreso" %}

    {% block title %}
        {{ parent() }} - {{ 'views.show.show'|trans({'%entity%': 'UsuarioIngreso'}, 'JordiLlonchCrudGeneratorBundle') }}
    {% endblock %}

    {% block page %}

        <section id="view">
            <header>
                <h1>Ver Ingreso de Usuario - <span style="color: #0064cd"> {{ entity }}</span></h1>
            </header>
            {{ wo_render_breadcrumbs() }}
            <div class="row-fluid">
                <form class="form-horizontal">
                    <fieldset>
                        <div class="control-group">
                            <label class="control-label">Id</label>

                            <div class="controls"><span class="input-xlarge uneditable-input">{{ entity.id }}</span>
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Lunes</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.lunes == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Martes</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.martes == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Miercoles</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.miercoles == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Jueves</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.jueves == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Viernes</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.viernes == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Sabado</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.sabado == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Domingo</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.domingo == true %}SI{% endif %}</span></div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Lugar_ingreso</label>

                            <div class="controls"><span
                                    class="input-xlarge uneditable-input">{% if entity.lugaringreso == true %}SI{% endif %}</span></div>
                        </div>
                    </fieldset>
                </form>

                <div>
                    <div class="float-left">
                        <a class="btn" href="{{ path('usuarioingreso') }}">
                            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
                        </a>
                    </div>
                    <div class="float-left">
                        <a class="btn btn-primary" href="{{ path('usuarioingreso_edit', { 'id': entity.id }) }}">
                            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
                        </a>
                    </div>
                    <div class="float-left">
                        <form action="{{ path('usuarioingreso_delete', { 'id': entity.id }) }}" method="post">
                            <input type="hidden" name="_method" value="DELETE"/>
                            {{ form_widget(delete_form) }}
                            <button class="btn btn-danger" type="submit"
                                    onclick="return confirm('{{ 'views.recordactions.confirm'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    {% endblock %}